    def clear_input(self,event=None):
        global txt
        self.link_s.delete(0,END)
        file = open("history.txt","w")
        file.write(" ")
        file.close()
        self.values = (" ")
        txt = Text(self,width=50,height=25)
        txt.pack()
        hf_o = open('history.txt','r').readlines()
        if self.next['state'] == 'normal' or self.back['state'] == 'normal':
            self.next['state'] = 'disabled' 
            self.back['state'] = 'disabled'
        else:
            pass
        for t in hf_o:
            txt['state'] = 'normal'
            txt.insert(END,str(t))
            txt['state'] = 'disabled'
    def clear_input_2(self,event=None):
        self.values = ("")
        # self.next['state'] = 'disabled'
        # self.back['state'] = 'disabled'
        file = open("history.txt","w")
        file.write("")
        file.close()
        self.link_s.configure(values=self.values)
        self.back_b['state'] = 'disabled'
    def clear_input_3(self,event=None):
        self.values = ("")
        self.link_s.config(values=self.values)
        # self.next['state'] = 'disabled'
        # self.back['state'] = 'disabled'
        file = open("history.txt","w")
        file.write("")
        file.close()
        file_2 = open("history.txt","r").readlines()
        # print(line)
        txt['state'] = 'normal'
        txt.delete("1.0",END)
        txt['state'] = 'disabled'
        for line_ in file_2:
            txt['state'] = 'normal'
            txt.insert(INSERT,str(line_))
            txt['state'] = 'disabled'
        # self.back_b['state'] = 'disabled'
