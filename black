    def about(self,event=None):
        try:
            self.about_frame = tkinterweb.HtmlFrame(self)
            self.about_frame.load_website("https://blacksoftware.ir")
            self.about_frame.pack(fill='x',expand=True)
        except (AttributeError,Exception,):
            print(False)
