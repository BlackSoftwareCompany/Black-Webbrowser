    def signup(self):
        global window_signup
        window_signup = Tk()
        window_signup.title('Black-Webbrowser/SignUp')
        window_signup.attributes("-toolwindow",True)
        label_s_u = Label(window_signup,text='Username:')
        label_s_u.place(bordermode=INSIDE,x=80,y=30)
        label_s_p = Label(window_signup,text='Password:')
        label_s_p.place(bordermode=INSIDE,x=80,y=70)
        self.username = Entry(window_signup,width=30,borderwidth=2)
        self.username.place(bordermode=INSIDE,x=145,y=30)
        self.password = Entry(window_signup,width=30,borderwidth=2,show="*")
        self.password.place(bordermode=INSIDE,x=145,y=70)
        login_b = Button(window_signup,text='SignUp',width=9,height=3,command=self.create_acc)
        login_b.place(bordermode=OUTSIDE,x=195,y=110)
        exit_b = Button(window_signup,text='Exit',width=9,height=3,command=window_signup.destroy)
        exit_b.place(bordermode=OUTSIDE,x=195,y=170)
        window_signup.bind("<Return>",self.create_acc)
        window_signup.geometry("500x400")
        window_signup.mainloop()
    def create_acc(self,event=None):
        self.username_f = open("username.txt","w")
        self.username_f.write(self.username.get())
        self.username_f.close()
        self.password_f = open("password.txt","w")
        self.password_f.write(self.password.get())
        self.password_f.close()
        window_signup.destroy()
        acc_file = open("acc","w")
        acc_file.write("True")
        acc_file.close()
        accountfile.add_command(label='Sign Out',command=self.sign_out)
        self.label_st.config(text=f'Webbrowser - {self.username_f}')
    def login_acc(self,event=None):
        username_f = open("username.txt","r+").read()
        password_f = open("password.txt","r+").read()
        if self.username.get() == username_f:
            if self.password.get() == password_f:
               self.label_st.config(text=f'Webbrowser - {self.username.get()}')
               acc_file = open("acc","r+")
               acc_file.write("signin")
               acc_file.close()
               self.window_black_c.destroy()
               accountfile.delete("Sign In")
               accountfile.add_command(label='Sign Out',command=self.sign_out)

            else:
                showerror(title='Cannot Login',message='Please, Check Password!')
                print(False)
        else:
            showerror(title='Cannot Login',message=f'Please, Check Username!')
            print(False)
    def login_fam(self,event=None):
        username_f = open("username.txt","r+").read()
        password_f = open("password.txt","r+").read()
        if self.username.get() == username_f:
            if self.password.get() == password_f:
               self.label_st.config(text=f'Webbrowser - {self.username.get()}')
               acc_file = open("acc","r+")
               acc_file.write("signin")
               acc_file.close()
               self.window_black_a.destroy()
               accountfile.delete("Sign In")
               accountfile.add_command(label='Sign Out')
            else:
                showerror(title='Cannot Login',message='Please, Check Password!')
                print(False)
        else:
            showerror(title='Cannot Login',message=f'Please, Check Username!')
            print(False)     
    def account(self):
        acc_file = open("acc","r+").read()
        if acc_file == 'True':
            self.window_black_c = Tk()
            self.window_black_c.title("Black-Account")
            self.username_l = Label(self.window_black_c,text='Username:',foreground='black',background='white')
            self.username_l.place(bordermode=INSIDE,x=80,y=30)
            self.password_l = Label(self.window_black_c,text='Password:',foreground='black',background='white')
            self.password_l.place(bordermode=INSIDE,x=80,y=70)
            self.username = Entry(self.window_black_c,width=30,borderwidth=2)
            self.username.place(bordermode=INSIDE,x=145,y=30)
            self.password = Entry(self.window_black_c,width=30,borderwidth=2,show="*")
            self.password.place(bordermode=INSIDE,x=145,y=70)
            login_b = Button(self.window_black_c,text='Login',width=9,height=3,command=self.login_acc)
            login_b.place(bordermode=OUTSIDE,x=200,y=110)
            exit_b = Button(self.window_black_c,text='Exit',width=9,height=3,command=self.window_black_c.destroy)
            exit_b.place(bordermode=OUTSIDE,x=200,y=170)
            self.window_black_c.bind("<Return>",self.login_acc)
            self.window_black_c.config(background='white')
            self.window_black_c.attributes("-toolwindow",True)
            self.window_black_c.geometry("500x400+50+50")
            self.window_black_c.mainloop()
        elif acc_file == 'signin':
            age_s = open("age-submit.txt","r+").read()
            if age_s == 'True':
                q = askyesno(title='Submit Age',message='Do you want to re-register?')
                if q:
                    age_s = open("age-submit.txt","r+")
                    age_s.write("False")
                    age_s.close()
                    self.account()
                else:
                    print(q)
            else:
                self.window_black_a = Tk()
                self.window_black_a.title("Black-Account")
                self.age = Entry(self.window_black_a,width=30,borderwidth=2)
                self.age.place(bordermode=INSIDE,x=145,y=30)
                login_b = Button(self.window_black_a,text='Submit',width=9,height=3,command=self.submit_age)
                login_b.place(bordermode=OUTSIDE,x=165,y=110)
                exit_b = Button(self.window_black_a,text='Exit',width=9,height=3,command=self.window_black_a.destroy)
                exit_b.place(bordermode=OUTSIDE,x=165,y=170)
                self.window_black_a.bind("<Return>",self.submit_age)
                self.window_black_a.config(background='white')
                self.window_black_a.attributes("-toolwindow",True)
                self.window_black_a.geometry("500x400+50+50")
                self.window_black_a.mainloop()
        else:
            self.signup()
    def submit_age(self):
        age_file = open("age.txt","r+")
        age_file.write(self.age.get())
        age_file.close()
        age_s = open("age-submit.txt","r+")
        age_s.write("True")
        age_s.close()
    def fam(self):
        acc_file = open("acc","r+").read()
        if acc_file == 'True':
            window_fam = Tk()
            window_fam.title("Family-Range")
            self.username_l = Label(window_fam,text='Username:',foreground='black',background='white')
            self.username_l.place(bordermode=INSIDE,x=80,y=30)
            self.password_l = Label(window_fam,text='Password:',foreground='black',background='white')
            self.password_l.place(bordermode=INSIDE,x=83,y=70)
            self.username = Entry(window_fam,width=30,borderwidth=2)
            self.username.place(bordermode=INSIDE,x=165,y=30)
            self.password = Entry(window_fam,width=30,borderwidth=2,show="*")
            self.password.place(bordermode=INSIDE,x=165,y=80)
            login_b = Button(window_fam,text='Login',width=9,height=3,command=self.login_fam)
            login_b.place(bordermode=OUTSIDE,x=205,y=118)
            exit_b = Button(window_fam,text='Exit',width=9,height=3,command=window_fam.destroy)
            exit_b.place(bordermode=OUTSIDE,x=205,y=178)
            window_fam.bind("<Return>",self.login_fam)
            window_fam.config(background='white')
            window_fam.attributes("-toolwindow",True)
            window_fam.geometry("500x400+50+50")
            window_fam.mainloop()
        elif acc_file == 'signin':
            showinfo(title='Family Mode',message='This will be the case in v2.0!')
            print("\nThis will be the case in v2.0!\n")
        else:
            self.signup()
    def ext5(self):
        window7.destroy()
        window7.quit()
    def history(self):
        global window6,txt
        window6 = Tk()
        window6.title('Black-Webbrowser/History')
        menu = Menu(window6)
        filemenu = Menu(menu,tearoff=0)
        filemenu.add_command(label='Clear',command=self.clear_input_3)
        filemenu.add_command(label='Close',accelerator='Ctrl+F4',command=self.close_1)
        menu.add_cascade(label='Option',menu=filemenu)
        window6.geometry("500x400")
        window6.resizable(False,False)
        window6.config(background='gray',menu=menu)
        txt = Text(window6,width=50,height=25)
        txt.pack()
        txt['state'] = 'disabled'
        hf_o = open('history.txt','r').readlines()
        for t in hf_o:
            txt['state'] = 'normal'
            txt.insert(END,str(t))
            txt['state'] = 'disabled'
        window6.mainloop()
    def close_1(self):
        window6.destroy()
    def search_5(self,x):
        self.search = True
        if self.check_tab == False:
            if self.link_s.get():
                self.window5 = Tk()
                self.window5.title('Black-Webbrowser')
                self.window5.geometry("700x500")
                self.window5.bind("<Alt-F3>",lambda x: self.black_3(x))
                self.f = Notebook(self.window5)
                self.tab1 = Frame(self.f)
                self.f.add(self.tab1,text=self.link_s.get())
                self.f.pack(fill='both',expand=True)
                self.check_tab = True
                menu = Menu(self.window5)

                filemenu = Menu(menu,tearoff=0)
                filemenu.add_command(label='Black',accelerator='Alt+F3',command=self.black_2)
                filemenu.add_command(label='Exit',accelerator='Alt+F4',command=self.ext7)
                menu.add_cascade(label='Option',menu=filemenu)
                self.window5.config(menu=menu)
                try:
                  self.frame = tkinterweb.HtmlFrame(self.tab1)
                  self.frame.load_website(self.link_s.get())
                  self.frame.pack(fill='x',expand=True)
                  hf = open('history.txt','a')
                  hf.write(str(self.link_s.get()))
                  hf.close()
                except:
                    showerror(title='Not Found',message='Item Not Found!')
                    print(False)
            # self.window5.mainloop()
        else:
            if self.check_tab == True:
                # self.f = Notebook(self.window5)
                self.tab2 = Frame(self.f)
                self.f.add(self.tab2,text=self.link_s.get())
                self.f.pack(fill='both',expand=True)
                self.frame_2 = tkinterweb.HtmlFrame(self.tab2)
                self.frame_2.load_website(self.link_s.get())
                self.frame_2.pack(fill='x',expand=True)
        self.window5.mainloop()
    def search(self,event=None):
        self.search = True # Code
        if self.link_s.get():
            if self.check_search_tab == False:
                self.bind("<Alt-F3>",lambda x: self.black_3(x))
                self.link_ss = self.link_s.get()
                try:
                        if self.back == True or self.next == True:
                            self.frame.destroy()
                    # self.key_search.destroy()
                        self.label_b.destroy()
                        self.about.destroy()
                        self.web.destroy()
                        self.dev.destroy()
                        self.exit.destroy()
                        self.back_b.destroy()
                        self.next_b.destroy()
                        try:
                            self.frame = tkinterweb.HtmlFrame(self.main_tab)
                            self.frame.load_website(self.link_ss)
                            self.frame.pack(fill='x',expand=True)
                        except (Exception,AttributeError,):
                            print(False)
                        self.all_search.append(self.link_s.get())
                        self.values = list(self.values)
                        self.values.append(self.ch.get())
                        self.values = tuple(self.values)
                        self.link_s.configure(values=self.values)
                        self.back_b_2 = Button(self.main_tab,text='Back',foreground='green',background='black',command=self.back_link)
                        self.back_b_2.place(bordermode=OUTSIDE,x=10,y=5)
                        self.next_b_2 = Button(self.main_tab,text='Next',foreground='green',background='black',command=self.next_link)
                        self.next_b_2.place(bordermode=OUTSIDE,x=50,y=5) 
                        self.black_b = Button(self.main_tab,background='black',foreground='green',text='Black',command=self.main_menu_s)
                        self.black_b.place(bordermode=OUTSIDE,x=650,y=3) 
                        self.next_b_2['state'] = 'disabled'
                        self.back_b_2['state'] = 'normal'
                        self.main_search = True
                        hf = open('history.txt','a')
                        hf.write(f'{self.link_ss}\n')
                        hf.close()
                        self.check_search_tab = True
                except (Exception,AttributeError,):
                    print(False)

            elif self.check_search_tab == True:        
                    if self.link_s.get():
                        self.bind("<Alt-F3>",lambda x: self.black_3(x))
                        # self.f = Notebook(self)
                        self.check_tab = True
                        try:
                           self.frame.destroy()
                           self.frame = tkinterweb.HtmlFrame(self.main_tab)
                           self.frame.load_website(self.link_s.get())
                           self.frame.pack(fill='x',expand=True)
                           self.all_search.append(self.link_s.get())
                           self.values = list(self.values) 
                           self.values.append(self.ch.get())
                           self.values = tuple(self.values)
                           self.link_s.configure(values=self.values)                      
                           self.back_b_2 = Button(self.main_tab,text='Back',command=self.back_link)
                           self.back_b_2.place(bordermode=OUTSIDE,x=10,y=5)
                           self.next_b_2 = Button(self.main_tab,text='Next',command=self.next_link)
                           self.check_search_tab = True
                           hf = open('history.txt','a')
                           hf.write(f'{self.link_s.get()}\n')
                           hf.close()
                        except (AttributeError,Exception,):
                            self.frame = tkinterweb.HtmlFrame(self.main_tab)
                            for link in self.link_s.get:
                               self.frame.load_website(f'https://www.google.com/search?{link}')
                            self.frame.pack(fill='x',expand=True)
                    else:
                        pass
        # elif self.key_search.get():
        #     if self.main_tab == True:
        #         self.bind("<Alt-F3>",lambda x: self.black_3(x))
        #         try:
        #             self.frame_main = tkinterweb.HtmlFrame(self.main_tab)
        #             self.frame_main.load_website(f'fhttps://www.google.com/search?q={self.key_search.get()}')
        #             self.frame_main.pack(fill='x',expand=True)
        #             self.back_b_2 = Button(self.main_tab,text='Back',command=self.back_link)
        #             self.back_b_2.place(bordermode=OUTSIDE,x=10,y=5)
        #             self.next_b_2 = Button(self.main_tab,text='Next',command=self.next_link)
        #             self.next_b_2.place(bordermode=OUTSIDE,x=50,y=5)  
        #             hf = open('history.txt','a')
        #             hf.write(f'{self.key_search.get()}\n')
        #             hf.close()
        #             self.check_search_tab = True
        #         except (Exception,):
        #             print(False)
        #     # self.window5.mainloop()
        #     else:
        #             self.bind("<Alt-F3>",lambda x: self.black_3(x))
        #             # self.f = Notebook(self)
        #             self.check_tab = True
        #             try:
        #             #    self.frame.destroy()
        #             #    self.key_search.destroy()
        #                self.label_b.destroy()
        #                self.frame_main = tkinterweb.HtmlFrame(self.main_tab)
        #                self.frame_main.load_website(self.key_search)
        #                self.frame_main.pack(fill='x',expand=True)
        #                self.back_b_2 = Button(self.main_tab,text='Back',command=self.back_link)
        #                self.back_b_2.place(bordermode=OUTSIDE,x=10,y=5)
        #                self.next_b_2 = Button(self.main_tab,text='Next',command=self.next_link)
