class argument(object):
      def arg(self):
          global window_arg,url
          try:
             if sys.argv[1].lower() == 'version' or sys.argv[1].lower() == '--version':
                 print("\nBlack-Webbrowser v1.5\n")
                 quit()
             elif sys.argv[1].lower() == 'help' or sys.argv[1].lower() == '--help':
                 print("""
Black-Webbrowser Argument:
          
          black
          black <url>
          black version or --version
          black help or --help\n""")
                 quit()
             elif sys.argv[1] == sys.argv[1]:
                url = str(sys.argv[1])
                try:
                    window_arg = Tk()
                    window_arg.title('Black-Webbrowser')
                    try:
                        frame_arg = tkinterweb.HtmlFrame(window_arg)
                        frame_arg.load_website(url)
                        frame_arg.pack(fill='both',expand=True)
                    except (Exception,):
                        print(False)
                    window_arg.geometry("600x500")
                    window_arg.mainloop()
                except (Exception,):
                    showerror(title='Cannot Running',message='Cannot Running Black-Webbrowser!')
                    print(False)
             else:
                 try:
                    r = requests.get('https://github.com')
                    if r.status_code == 200:
                        if platform.system() == 'Linux':
                            if os.getuid() == 0:
                                window = black_webbrowser()
                                window.title_b()
                                window.cls()
                                print(window.time_zone)
                                window.main()
                            else:
                                showerror(title='Cannot Running',message='Please, Check Root!')
                                print(False)
                        else:
                            window = black_webbrowser()
                            window.title_b()
                            window.cls()
                            print(window.time_zone)
                            window.main()
                 except (requests.ConnectTimeout,requests.exceptions.ConnectionError,Exception,):
                     showwarning(title='Cannot Connecting',message='Please, check Internet')
                     print(False)
          except (Exception,IndexError):
              window = black_webbrowser()
              window.title_b()
              window.cls()
              print(f'start At: {window.time_zone}')
              window.main()
class black_webbrowser_installing(Tk):
    def __init__(self):
        super(black_webbrowser_installing,self).__init__()
        self.title('Black-Webbrowser/Installing')
        self.photo = PhotoImage(file = './Scr/black.png')
        self.style = Style("cyborg").master
        i_menu = Menu(self)
        filemenu = Menu(i_menu,tearoff=0)
        filemenu.add_command(label='About',command=self.about)
        filemenu.add_command(label='Black',command=self.black)
        filemenu.add_separator()
        filemenu.add_command(label='Help',command=self.black_help)
        filemenu.add_separator()
        filemenu.add_command(label='Exit',command=self.destroy)
        label_l = Label(self,text='Black-Webbrowser',background='gray10',foreground='green',font=("None",30))
        label_l.place(bordermode=INSIDE,x=90,y=20)
        self.install_b = Button(self,text='Install',width=9,command=self.install)
        self.install_b.place(bordermode=OUTSIDE,x=200,y=110)
        self.exit_b = Button(self,text='Exit',width=9,command=self.ext)
        self.exit_b.place(bordermode=OUTSIDE,x=200,y=145)

        self.config(background='white')
        self.iconphoto(False,self.photo)
        self.resizable(False,False)
        self.config(background='gray10')
        self.geometry("500x400")
        self.mainloop()
    def black(self,event=None):
        win = Tk()
        win.title('Black-Webbrowser/Black')
        win.geometry("600x500")
        try:
            w = tkinterweb.HtmlFrame(win)
            w.load_website('https://black-software-com.github.io/Black-Software/')
            w.pack(fill='both',expand=True)
        except (Exception,):
            print(False)
            win.destroy()
        win.mainloop()
    def about(self,event=None):
        win = Tk()
        win.title('Black-Webbrowser/About')
        win.geometry("600x500")
        try:
            w = tkinterweb.HtmlFrame(win)
            w.load_website('https://github.com/black-software-com')
            w.pack(fill='both',expand=True)
        except (Exception,):
            print(False)
            win.destroy()
        win.mainloop()
    def black_help(self,event=None):
        win = Tk()
        win.title('Black-Webbrowser/Black-Help')
        win.geometry("600x500")
        try:
            w = tkinterweb.HtmlFrame(win)
            w.load_website('https://black-software-com.github.io/Black-Help')
            w.pack(fill='both',expand=True)
        except (Exception,):
            print(False)
            win.destroy()
        win.mainloop()
    def install(self):
        global pr
        pr = Progressbar(self,orient='horizontal',mode='determinate',length=200)
        pr.place(bordermode=INSIDE,x=145,y=73)
        pr.start(55)
        pr.after(6000,self.install_2)
    def install_2(self):
        subprocess.getoutput("chmod a+x black.py")
        pr.stop()
        pr.destroy()
        label_mess = Label(self,text='Complete!',foreground='green',background='gray10')
        label_mess.place(bordermode=INSIDE,x=210,y=75)
        self.install_b.destroy()
        self.start_b = Button(self,text='Start',width=9,command=self.start_program)
        self.start_b.place(bordermode=OUTSIDE,x=200,y=110)
    def start_program(self):
        self.destroy()
        window = black_webbrowser()
        window.title_b()
        window.cls()
        window.main()
    def ext(self):
        self.destroy()
        self.quit()
        quit()
if __name__ == '__main__':
    try:
        r = requests.get('https://google.com')
        if r.status_code == 200:
            installing_check = open("install_check","r+").read()
            if platform.system() == 'Linux':
               if os.getuid() == 0:
                   if installing_check == 'False' or installing_check == 'False\n':
                       os.system("bash /Core/install.sh")
                       window = black_webbrowser_installing()
                       installing_check = open("install_check","w")
                       installing_check.write("True")
                       installing_check.close()
                   elif installing_check == 'True' or installing_check == 'True\n':
                         arg = argument()
                         arg.arg()
                   else:
                       showerror(title='Cannot Runnig Black-Webbrowser',message='Cannot Running Black-Webbrowser!')
                       print(False)
                       sys.exit()
               else:
                   showerror(title='Cannot Running',message='Please, Run Black-Webbrowser as Root!')
                   print(False)
            else:
                if installing_check == 'False' or installing_check == 'False\n':
                    window = black_webbrowser_installing()
                    installing_check = open("install_check","w")
                    installing_check.write("True")
                    installing_check.close()
                elif installing_check == 'True' or installing_check == 'True\n':
                      arg = argument()
                      arg.arg()
                else:
                    showerror(title='Cannot Running',message='Cannot Running Black-Webbrowser!')
                    print(False)
                    sys.exit()
    except (requests.ConnectTimeout,requests.exceptions.ConnectionError,Exception,):
        showwarning(title='Cannot Connecting',message='Please, check Internet')
        print(False)
